{
    "name": "ozzie/today",
    "description": "CLI tool for today.ozzie.sh",
    "keywords": [
        "console",
        "cli",
        "task",
        "todo"
    ],
    "homepage": "https://today.ozzie.sh",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-zero/laravel-zero/issues",
        "source": "https://github.com/laravel-zero/laravel-zero"
    },
    "authors": [
        {
            "name": "Ozzie Neher",
            "email": "oswaldy@hey.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "guzzlehttp/guzzle": "^7.1",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.6",
        "laravel-zero/framework": "^8.0",
        "padraic/phar-updater": "^1.0.6"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/today"
    ]
}
